<h1>将棋対戦</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <% if logged_in? %>
      <%= form_with(url: matchs_path(user_id: @user.id), local: true) do |f| %>
        <%= f.submit '対局室へ移動', class: 'btn btn-primary btn-block', id: 'matchRoom' %>
      <% end %>
    <% else %>
      <%= link_to "ログイン", login_path, class: 'btn btn-primary btn-block'%>
    <% end %>
  </div>
</div>
